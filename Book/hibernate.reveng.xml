<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="donate" catalog="users" class="com.tao.model.Donate">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="orders" catalog="users" class="com.tao.model.Orders">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="publishment" catalog="users"
		class="com.tao.model.Publishment">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="userrs" catalog="users" class="com.tao.model.Userrs">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>